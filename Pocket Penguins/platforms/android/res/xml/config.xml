<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1400009" id="air.pocketpenguins" version="3.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="AndroidFullScreen">
        <param name="android-package" value="com.mesmotronic.plugins.FullScreenPlugin" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="Insomnia">
        <param name="android-package" value="nl.xservices.plugins.Insomnia" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Pocket Penguins</name>
    <description>
        Warning: Cute alert! An intimate view into our live African penguin exhibit, streaming in real-time. 
    </description>
    <author email="webcams@calacademy.org" href="https://www.calacademy.org">
        California Academy of Sciences
    </author>
    <content src="index.html" />
    <access origin="*" />
    <splash src="splash.png" />
    <icon src="icon.png" />
    <icon height="48" qualifier="mdpi" src="icons/mdpi.png" width="48" />
    <icon height="72" qualifier="hdpi" src="icons/hdpi.png" width="72" />
    <icon height="96" qualifier="xhdpi" src="icons/xhdpi.png" width="96" />
    <icon height="144" qualifier="xxhdpi" src="icons/xxhdpi.png" width="144" />
    <icon height="192" qualifier="xxxhdpi" src="icons/xxxhdpi.png" width="192" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="orientation" value="landscape" />
</widget>
